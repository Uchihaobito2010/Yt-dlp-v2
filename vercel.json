{
  "functions": {
    "api/*.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/health" },
    { "source": "/api/download", "destination": "/api/download.js" },
    { "source": "/api/health", "destination": "/api/health.js" },
    { "source": "/api/serve", "destination": "/api/serve.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Developer",
          "value": "Paras Chourasiya"
        },
        {
          "key": "X-Contact",
          "value": "Telegram: @Aotpy"
        },
        {
          "key": "X-Service",
          "value": "Instagram Downloader API"
        },
        {
          "key": "X-Warning",
          "value": "Personal use only. Contact @Aotpy for issues."
        }
      ]
    }
  ]
}
